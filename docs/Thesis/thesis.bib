journal = {Computer Graphics Forum},
keywords = {1 digitization and,10 vision and scene,2,3,3d scanning,4,5 computational geometry and,7 three-dimensional graphics and,acm css,and texture,color,geometric algorithms,i,languages and systems,mesh generation,modeling and recovery of,object modeling,physical attributes,range images,realism,reflectance models,shading,shadowing,shape,texture,texture maps sensor fusion,understanding},
