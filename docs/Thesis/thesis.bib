@article{Jeffery2011,
abstract = {A central theme in the study of animal navigation has been the extent to which it requires the formation of an internal representation of space, the so-called ‘cognitive map’. Although its properties remain disputed, it is now generally accepted that a map-like representations exists in the brain, and neurobiological studies, conducted in tandem with behavioural investigations, have done much to elucidate the neural substrate of the map as it operates in two dimensions. However, little is known about how it encodes real-world, three-dimensional space. The present chapter uses recent neurobiological and behavioural findings to explore this question. It will be argued that the navigational problems in three dimensions are qualitatively as well as quantitatively different from those in two, and evidence will be reviewed suggesting that – perhaps for this reason – horizontal and vertical space are processed separately in the vertebrate brain. The possible adaptive consequences of such an anisotropic encoding scheme are outlined.},
author = {Jeffery, K},
file = {:home/rickert/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Jeffery - 2011 - Navigating in a 3D world.pdf:pdf},
keywords = {Spatial cognition, Navigation, Hippocampus, Entorh},
title = {{Navigating in a 3D world}},
url = {http://discovery.ucl.ac.uk/1312150/},
year = {2011}
}
